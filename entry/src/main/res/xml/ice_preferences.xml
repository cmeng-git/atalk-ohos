<?xml version="1.0" encoding="utf-8"?>

<PreferenceScreen xmlns:ohos="http://schemas.huawei.com/res/ohos">

    <PreferenceCategory
        ohos:key="pref.cat.jbr.ice.general"
        ohos:title="$string:general">

        <CheckBoxPreference
            ohos:key="pref_key_ice_enabled"
            ohos:title="$string:jbr_use_ice" />

        <CheckBoxPreference
            ohos:key="pref_key_upnp_enabled"
            ohos:title="$string:jbr_use_upnp" />

        <CheckBoxPreference
            ohos:key="pref_key_auto_discover_stun"
            ohos:title="$string:jbr_auto_stun_discovery" />

        <Preference
            ohos:key="pref_key_stun_turn_servers"
            ohos:summary="$string:jbr_stun_list_summary"
            ohos:title="$string:jbr_stun_list" />
    </PreferenceCategory>

    <PreferenceCategory
        ohos:key="pref.cat.jbr.jinglenodes"
        ohos:title="$string:jbr_jingle_nodes">
        <CheckBoxPreference
            ohos:key="pref_key_use_jingle_nodes"
            ohos:title="$string:jbr_jingle_use" />

        <CheckBoxPreference
            ohos:key="pref_key_auto_relay_discovery"
            ohos:title="$string:jbr_jingle_auto" />

        <Preference
            ohos:key="pref_key_jingle_node_list"
            ohos:summary="$string:jbr_jingle_list_summary"
            ohos:title="$string:jbr_jingle_list" />
    </PreferenceCategory>
</PreferenceScreen>