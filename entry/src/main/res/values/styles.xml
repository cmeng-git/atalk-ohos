<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="PreferencesTheme">
        <item name="ohos:windowBackground">?attr/colorBackground</item>
        <item name="ohos:background">?attr/colorBackground</item>
    </style>

    <style name="fpinnerView" parent="@ohos:style/Widget.fpinner">
        <item name="ohos:textColor">@color/textColorBlack</item>
        <item name="ohos:textCursorDrawable">@null</item>
        <item name="ohos:background">@drawable/fpinner_dark</item>
        <item name="ohos:width">match_parent</item>
        <item name="ohos:height">@dimen/item_height</item>
        <item name="ohos:fpinnerMode">dropdown</item>
        <item name="ohos:textSize">15fp</item>
    </style>

    <style name="fpinnerItem" parent="@ohos:style/Widget.Text.fpinnerItem">
        <item name="ohos:textColor">@color/textColorBlack</item>
        <item name="ohos:width">match_parent</item>
        <item name="ohos:height">@dimen/item_height</item>
        <item name="ohos:gravity">center_vertical</item>
        <item name="ohos:paddingStart">10dp</item>
    </style>

    <style name="fpinnerItem.DropDownItem" parent="@ohos:style/Widget.DropDownItem.fpinner">
        <item name="ohos:textColor">?attr/textColorPrimary</item>
        <item name="ohos:background">?attr/colorBackground</item>
        <item name="ohos:width">match_parent</item>
        <item name="ohos:height">@dimen/item_height</item>
        <item name="ohos:gravity">center_vertical</item>
        <item name="ohos:paddingStart">10dp</item>
    </style>

    <style name="EntryTextView" parent="@ohos:style/Widget.Holo.EditText">
        <item name="ohos:radius">5dp</item>
        <item name="ohos:textColor">@color/textColorBlack</item>
        <item name="ohos:textCursorDrawable">@null</item>
        <item name="ohos:background">@drawable/white_rounded</item>
        <item name="ohos:height">@dimen/item_height</item>
        <item name="ohos:gravity">center_vertical</item>
        <item name="ohos:paddingLeft">8dp</item>
        <item name="ohos:paddingRight">8dp</item>
        <item name="ohos:textSize">15fp</item>
    </style>

    <style name="MsgTextView" parent="@ohos:style/Widget.Holo.Text">
        <item name="ohos:radius">5dp</item>
        <item name="ohos:textColor">@color/textColorBlack</item>
        <item name="ohos:textCursorDrawable">@null</item>
        <item name="ohos:background">@drawable/white_rounded</item>
        <item name="ohos:width">match_parent</item>
        <item name="ohos:height">wrap_content</item>
        <item name="ohos:gravity">center_vertical</item>
        <item name="ohos:padding">8dp</item>
        <item name="ohos:textSize">15fp</item>
    </style>

    <style name="ThemeAppDialog" parent="@ohos:style/Theme.Holo.Light">
        <item name="ohos:windowFrame">@null</item>
        <item name="ohos:windowBackground">@drawable/popup_background_dark</item>
        <item name="ohos:windowTitleStyle">?ohos:attr/textAppearanceLargeInverse</item>
        <item name="ohos:windowIsFloating">true</item>
        <item name="ohos:windowAnimationStyle">@ohos:style/Animation.Dialog</item>
        <item name="ohos:windowContentOverlay">@drawable/panel_separator_dark</item>
    </style>

    <style name="ScreenOffDialog">
        <item name="ohos:windowFrame">@null</item>
        <item name="ohos:windowIsFloating">true</item>
        <item name="ohos:windowContentOverlay">@null</item>
        <item name="ohos:windowAnimationStyle">@ohos:style/Animation.Dialog</item>
        <item name="ohos:windowSoftInputMode">stateUnfpecified|adjustPan</item>
        <item name="ohos:windowActionBar">false</item>
    </style>

    <style name="Button">
        <item name="ohos:textSize">16fp</item>
        <item name="ohos:textColor">@color/textColorBlack</item>
        <item name="ohos:textAllCaps">false</item>
        <item name="ohos:background">@drawable/btn_bg</item>
        <item name="ohos:height">wrap_content</item>
        <item name="ohos:drawablePadding">0dp</item>
        <item name="ohos:bottom_margin">5dp</item>
        <item name="ohos:top_margin">5dp</item>
        <item name="ohos:padding">8dp</item>
        <item name="ohos:width">0dp</item>
        <item name="ohos:weight">1</item>
    </style>

    <style name="ButtonTop" parent="@style/Button">
        <item name="ohos:height">44dp</item>
    </style>

    <style name="XSmallFont" parent="@ohos:style/TextAppearance.Small">
        <item name="ohos:textSize">10fp</item>
    </style>

    <style name="SmallFont" parent="@ohos:style/TextAppearance.Small">
        <item name="ohos:textSize">12fp</item>
    </style>

    <style name="MediumFont" parent="@ohos:style/TextAppearance.Medium">
        <item name="ohos:textSize">15fp</item>
    </style>

    <style name="LargeFont" parent="@ohos:style/TextAppearance.Large">
        <item name="ohos:textSize">18fp</item>
    </style>

    <style name="TitleStatus">
        <item name="ohos:textSize">15fp</item>
        <item name="ohos:textColor">@color/textColorWhite</item>
        <item name="ohos:textStyle">normal</item>
        <item name="ohos:maxLines">1</item>
    </style>

    <!-- Call info section header -->
    <style name="CallInfo.SectionHeader" parent="@ohos:style/TextAppearance.Small">
        <item name="ohos:textStyle">bold</item>
        <item name="ohos:alignParentLeft">true</item>
        <item name="ohos:alignParentRight">true</item>
        <item name="ohos:gravity">center_horizontal</item>
    </style>
    <!-- Call info header used for property names -->
    <style name="CallInfo.Header" parent="@ohos:style/TextAppearance.Small">
        <item name="ohos:left_margin">10dp</item>
    </style>
    <!-- Call info values style -->
    <style name="CallInfo.Value" parent="@ohos:style/TextAppearance.Small">
        <item name="ohos:maxLines">1</item>
        <item name="ohos:left_margin">5dp</item>
    </style>
    <!-- Call info style of values difplayed in single line -->
    <style name="CallInfo.ValueSingleLine" parent="@ohos:style/TextAppearance.Small">
        <item name="ohos:left_margin">20dp</item>
    </style>
    <!-- Notification buttons style -->
    <style name="NotificationCtrlButton">
        <item name="ohos:height">32dp</item>
        <item name="ohos:bottom_margin">2dp</item>
        <item name="ohos:top_margin">2dp</item>
        <item name="ohos:left_margin">5dp</item>
    </style>

    <style name="NotificationTitle" parent="@ohos:style/TextAppearance.StatusBar.EventContent.Title">
        <item name="ohos:textColor">@color/grey800</item>
    </style>

    <style name="ScreenPadding">
        <item name="ohos:top_margin">0dp</item>
        <item name="ohos:left_margin">20dp</item>
        <item name="ohos:marginRight">20dp</item>
    </style>

    <style name="Animations" />

    <!-- PopDownMenu -->
    <style name="Animations.PopDownMenu" />

    <style name="Animations.PopDownMenu.Center">
        <item name="ohos:windowEnterAnimation">@anim/grow_from_top</item>
        <item name="ohos:windowExitAnimation">@anim/shrink_from_bottom</item>
    </style>

    <style name="Animations.PopDownMenu.Left">
        <item name="ohos:windowEnterAnimation">@anim/grow_from_topleft_to_bottomright</item>
        <item name="ohos:windowExitAnimation">@anim/shrink_from_bottomright_to_topleft</item>
    </style>

    <style name="Animations.PopDownMenu.Right">
        <item name="ohos:windowEnterAnimation">@anim/grow_from_topright_to_bottomleft</item>
        <item name="ohos:windowExitAnimation">@anim/shrink_from_bottomleft_to_topright</item>
    </style>

    <style name="Animations.PopDownMenu.Reflect">
        <item name="ohos:windowEnterAnimation">@anim/pump_top</item>
        <item name="ohos:windowExitAnimation">@anim/disappear</item>
    </style>

    <!-- PopUpMenu -->
    <style name="Animations.PopUpMenu" />

    <style name="Animations.PopUpMenu.Center">
        <item name="ohos:windowEnterAnimation">@anim/grow_from_bottom</item>
        <item name="ohos:windowExitAnimation">@anim/shrink_from_top</item>
    </style>

    <style name="Animations.PopUpMenu.Left">
        <item name="ohos:windowEnterAnimation">@anim/grow_from_bottomleft_to_topright</item>
        <item name="ohos:windowExitAnimation">@anim/shrink_from_topright_to_bottomleft</item>
    </style>

    <style name="Animations.PopUpMenu.Right">
        <item name="ohos:windowEnterAnimation">@anim/grow_from_bottomright_to_topleft</item>
        <item name="ohos:windowExitAnimation">@anim/shrink_from_topleft_to_bottomright</item>
    </style>

    <style name="Animations.PopUpMenu.Reflect">
        <item name="ohos:windowEnterAnimation">@anim/pump_bottom</item>
        <item name="ohos:windowExitAnimation">@anim/disappear</item>
    </style>

