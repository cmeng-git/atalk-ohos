<?xml version="1.0" encoding="UTF-8" ?>
<shape
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:shape="rectangle">

    <solid
        ohos:color="#22000000"/>

    <gradient
        ohos:shader_type="linear_gradient"
        ohos:start_angle="270"
        ohos:background_start_color="$color:blue100"
        ohos:background_end_color="$color:white" />

    <stroke
        ohos:width="1vp"
        ohos:color="$color:grey"/>

    <corners
        ohos:radius="10vp"/>
</shape>