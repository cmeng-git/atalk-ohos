<?xml version="1.0" encoding="utf-8"?>

<DirectionalLayout xmlns:ohos="http://schemas.huawei.com/res/ohos"
    xmlns:custom="http://schemas.huawei.com/apk/res-auto"
    ohos:id="$+id:viewContent"
    ohos:width="match_parent"
    ohos:height="match_parent"
    ohos:alignment="vertical_center"
    ohos:orientation="vertical">

    <org.atalk.ohos.gui.widgets.TouchInterceptor
        ohos:id="$+id:encodingList"
        ohos:width="match_parent"
        ohos:height="0vp"
        ohos:weight="1"
        custom:dragRegionEndX="$float:enc_list_drag_e_x"
        custom:dragRegionStartX="$float:enc_list_drag_s_x"
        custom:itemExpandedHeight="$float:expanded_item_height"
        custom:itemHeight="$float:list_item_height" />

</DirectionalLayout>