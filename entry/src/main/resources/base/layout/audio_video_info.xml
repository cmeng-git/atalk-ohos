<?xml version="1.0" encoding="utf-8"?>

<DependentLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:height="match_parent"
    ohos:width="match_parent">

    <Text
        ohos:id="$+id:audioVideoLabel"
        ohos:height="match_content"
        ohos:width="match_parent"
        ohos:background_element="$graphic:call_status_bg"
        ohos:text="$string:callinfo_audio_info"
        ohos:text_alignment="horizontal_center"
        ohos:text_weight="$float:text_bold"/>

    <Text
        ohos:id="$+id:mediaStreamLabel"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:audioVideoLabel"
        ohos:margin="10vp"
        ohos:text="$string:callinfo_media_stream_transport_protocol"/>

    <Text
        ohos:id="$+id:mediaTransport"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:mediaStreamLabel"
        ohos:max_text_lines="1"
        ohos:start_margin="20vp"
        ohos:text="XXX / XXX (Key exchange protocol: XXX YYY-ZZ-XXX/YYY)"/>

    <Text
        ohos:id="$+id:videoSizeLabel"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:mediaTransport"
        ohos:margin="10vp"
        ohos:text="$string:callinfo_video_size"/>

    <Text
        ohos:id="$+id:videoSize"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:mediaTransport"
        ohos:end_of="$id:videoSizeLabel"
        ohos:text="↓ YYxYY ↑ YYxYY"
        ohos:start_margin="5vp"
        ohos:max_text_lines="1"/>

    <Text
        ohos:id="$+id:codecLabel"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:videoSizeLabel"
        ohos:margin="10vp"
        ohos:text="$string:callinfo_codec"/>

    <Text
        ohos:id="$+id:codec"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:codecLabel"
        ohos:max_text_lines="1"
        ohos:start_margin="20vp"
        ohos:text="Xxxxx / Yyyyy Hz"/>

    <Text
        ohos:id="$+id:iceExtTypeLabel"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:codec"
        ohos:margin="10vp"
        ohos:multiple_lines="true"
        ohos:text="$string:callinfo_candidate_extended_type"/>

    <Text
        ohos:id="$+id:iceExtType"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:iceExtTypeLabel"
        ohos:max_text_lines="1"
        ohos:start_margin="20vp"
        ohos:text="ICE ext cand type"/>

    <Text
        ohos:id="$+id:localHostLabel"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:iceExtType"
        ohos:margin="10vp"
        ohos:text="$string:callinfo_localhost_address"/>

    <Text
        ohos:id="$+id:iceLocalHost"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:localHostLabel"
        ohos:max_text_lines="1"
        ohos:start_margin="20vp"
        ohos:text="xxx.xxx.xxx.xxx / xxxx"/>

    <Text
        ohos:id="$+id:iceRemoteHostLabel"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:iceLocalHost"
        ohos:margin="10vp"
        ohos:text="$string:callinfo_ice_remote_host_address"/>

    <Text
        ohos:id="$+id:iceRemoteHost"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:iceRemoteHostLabel"
        ohos:max_text_lines="1"
        ohos:start_margin="20vp"
        ohos:text="xxx.xxx.xxx.xxx / xxxx"/>

    <Text
        ohos:id="$+id:iceLocalReflxLabel"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:iceRemoteHost"
        ohos:margin="10vp"
        ohos:multiple_lines="true"
        ohos:text="$string:callinfo_ice_local_reflexive_address"/>

    <Text
        ohos:id="$+id:iceLocalReflx"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:iceLocalReflxLabel"
        ohos:max_text_lines="1"
        ohos:start_margin="20vp"
        ohos:text="xxx.xxx.xxx.xxx / xxxx"/>

    <Text
        ohos:id="$+id:iceLocalRelayedLabel"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:iceLocalReflx"
        ohos:margin="10vp"
        ohos:multiple_lines="true"
        ohos:text="$string:callinfo_ice_local_relayed_address"/>

    <Text
        ohos:id="$+id:iceLocalRelayed"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:iceLocalRelayedLabel"
        ohos:max_text_lines="1"
        ohos:start_margin="20vp"
        ohos:text="xxx.xxx.xxx.xxx / xxxx"/>

    <Text
        ohos:id="$+id:iceRemoteRelayedLabel"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:iceLocalRelayed"
        ohos:margin="10vp"
        ohos:multiple_lines="true"
        ohos:text="$string:callinfo_ice_remote_relayed_address"/>

    <Text
        ohos:id="$+id:iceRemoteRelayed"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:iceRemoteRelayedLabel"
        ohos:text="xxx.xxx.xxx.xxx / xxxx"/>

    <Text
        ohos:id="$+id:iceRemoteReflxLabel"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:iceRemoteRelayed"
        ohos:margin="10vp"
        ohos:multiple_lines="true"
        ohos:text="$string:callinfo_ice_remote_reflexive_address"/>

    <Text
        ohos:id="$+id:iceRemoteReflexive"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:iceRemoteReflxLabel"
        ohos:max_text_lines="1"
        ohos:start_margin="20vp"
        ohos:text="xxx.xxx.xxx.xxx / xxxx"/>

    <Text
        ohos:id="$+id:localIpLabel"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:iceRemoteReflexive"
        ohos:margin="10vp"
        ohos:multiple_lines="true"
        ohos:text="$string:callinfo_local_ip"/>

    <Text
        ohos:id="$+id:localIp"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:localIpLabel"
        ohos:max_text_lines="1"
        ohos:start_margin="20vp"
        ohos:text="xxx.xxx.xxx.xxx / xxxx"/>

    <Text
        ohos:id="$+id:remoteIpLabel"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:localIp"
        ohos:margin="10vp"
        ohos:multiple_lines="true"
        ohos:text="$string:callinfo_remote_ip"/>

    <Text
        ohos:id="$+id:remoteIp"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:remoteIpLabel"
        ohos:max_text_lines="1"
        ohos:start_margin="20vp"
        ohos:text="xxx.xxx.xxx.xxx / xxxx"/>

    <Text
        ohos:id="$+id:bandwidthLabel"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:remoteIp"
        ohos:margin="10vp"
        ohos:text="$string:callinfo_bandwidth"/>

    <Text
        ohos:id="$+id:bandwidth"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:remoteIp"
        ohos:end_of="$id:bandwidthLabel"
        ohos:text="↓ xx Kbps ↑ xx Kbps"
        ohos:start_margin="5vp"
        ohos:max_text_lines="1"/>

    <Text
        ohos:id="$+id:lossRateLabel"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:bandwidthLabel"
        ohos:margin="10vp"
        ohos:text="$string:callinfo_loss_rate"/>

    <Text
        ohos:id="$+id:lossRate"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:bandwidthLabel"
        ohos:end_of="$id:lossRateLabel"
        ohos:text="↓ xx% ↑ xx%"
        ohos:start_margin="5vp"
        ohos:max_text_lines="1"/>

    <Text
        ohos:id="$+id:decodedWithFecLabel"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:lossRateLabel"
        ohos:margin="10vp"
        ohos:text="$string:callinfo_decoded_with_fec"/>

    <Text
        ohos:id="$+id:decodedWithFEC"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:lossRateLabel"
        ohos:end_of="$id:decodedWithFecLabel"
        ohos:text="xx"
        ohos:start_margin="5vp"
        ohos:max_text_lines="1"/>

    <Text
        ohos:id="$+id:discardedPercentLabel"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:decodedWithFecLabel"
        ohos:margin="10vp"
        ohos:text="$string:callinfo_discarded_percent"/>

    <Text
        ohos:id="$+id:discardedPercent"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:decodedWithFecLabel"
        ohos:end_of="$id:discardedPercentLabel"
        ohos:text="xx%"
        ohos:start_margin="5vp"
        ohos:max_text_lines="1"/>

    <Text
        ohos:id="$+id:discardedTotalLabel"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:discardedPercentLabel"
        ohos:margin="10vp"
        ohos:text="$string:callinfo_discarded_total"/>

    <Text
        ohos:id="$+id:discardedTotal"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:discardedTotalLabel"
        ohos:max_text_lines="1"
        ohos:start_margin="20vp"
        ohos:text="xx (xx late, xx full, xx shrink, xx reset)"/>

    <Text
        ohos:id="$+id:adaptJitterLabel"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:discardedTotal"
        ohos:margin="10vp"
        ohos:text="$string:callinfo_adaptive_jitter_buffer"/>

    <Text
        ohos:id="$+id:adaptiveJitterBuffer"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:discardedTotal"
        ohos:end_of="$id:adaptJitterLabel"
        ohos:text="enabled"
        ohos:start_margin="5vp"
        ohos:max_text_lines="1"/>

    <Text
        ohos:id="$+id:jitterBuffLabel"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:adaptJitterLabel"
        ohos:margin="10vp"
        ohos:text="$string:callinfo_ice_state_jitter_buffer_delay"/>

    <Text
        ohos:id="$+id:jitterBuffer"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:jitterBuffLabel"
        ohos:max_text_lines="1"
        ohos:start_margin="20vp"
        ohos:text="~xxms; currently in queue: xx/xx packets"/>

    <Text
        ohos:id="$+id:rttLabel"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:jitterBuffer"
        ohos:margin="10vp"
        ohos:text="$string:callinfo_rtt"/>

    <Text
        ohos:id="$+id:RTT"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:jitterBuffer"
        ohos:end_of="$id:rttLabel"
        ohos:text="xx ms"
        ohos:start_margin="5vp"
        ohos:max_text_lines="1"/>

    <Text
        ohos:id="$+id:jitterLabel"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:rttLabel"
        ohos:margin="10vp"
        ohos:multiple_lines="true"
        ohos:text="$string:callinfo_jitter"/>

    <Text
        ohos:id="$+id:jitter"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:below="$id:rttLabel"
        ohos:end_of="$id:jitterLabel"
        ohos:text="↓ xx ms ↑ xx ms"
        ohos:start_margin="5vp"
        ohos:max_text_lines="1"/>
</DependentLayout>