<?xml version="1.0" encoding="utf-8"?>

<DirectionalLayout xmlns:ohos="http://schemas.huawei.com/res/ohos"
    xmlns:custom="http://schemas.huawei.com/apk/res-auto"
    ohos:width="match_parent"
    ohos:height="match_parent"
    ohos:orientation="vertical">

    <Text
        ohos:id="$+id:text1"
        ohos:text_size="$float:medium_font"
        ohos:width="match_content"
        ohos:height="match_content"
        ohos:bottom_margin="5vp"
        ohos:start_margin="15vp"
        ohos:top_margin="5vp"
        ohos:text="$string:sec_protocols_message" />

    <org.atalk.ohos.gui.widgets.TouchInterceptor
        ohos:id="$+id:list"
        ohos:width="match_parent"

        ohos:height="0vp"
        ohos:weight="1"

        custom:dragRegionEndX="$float:enc_list_drag_e_x"
        custom:dragRegionStartX="$float:enc_list_drag_s_x"
        custom:itemExpandedHeight="$float:expanded_item_height"
        custom:itemHeight="$float:list_item_height" />
</DirectionalLayout>